//import  tools bar
import { Toolbar } from '@material-ui/core';
//import style for inline styling
import { makeStyles } from '@material-ui/core/styles';

// create the navigation and pass the probs to it 
export default function Navigationbar(props){
//now ehat will it return app bar return ()
return(

<AppBar><Toolbar> hello123  </Toolbar> </AppBar>
	)

}


2.now go to app.js and import the theme as well as the navigation  and creare theme i ui/theme.js

import { ThemeProvider } from '@material-ui/core/styles';
import { makeStyles } from '@material-ui/core/styles';
import Navigationbar from '../components/ui/navigationbar';
import theme from './ui/Theme';


3.in retrun function() call it 

 <ThemeProvider theme={theme}>
  <Navigationbar />
</ThemeProvider>

